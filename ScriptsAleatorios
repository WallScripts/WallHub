--Funções Copiar (slc pai tá muito especialista)

local function copyToClipboard(text)
    setclipboard(text)
    OrionLib:MakeNotification({
        Name = "Copiado",
        Content = text .. " foi copiado para a área de transferência.",
        Image = "rbxassetid://7733911828",
        Time = 5
    })
end

